{
  "name": "nova-packages-tool",
  "private": true,
  "scripts": {
    "dev": "npm run nova-install && npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run nova-install && npm run production",
    "production": "mix --production",
    "nova-install": "npm --prefix='./vendor/laravel/nova' ci"
  },
  "dependencies": {
    "@babel/preset-env": "7.23.9",
    "@vue/babel-plugin-jsx": "1.1.5",
    "@vue/babel-preset-jsx": "1.4.0",
    "@vue/compiler-sfc": "3.4.19",
    "laravel-mix": "6.0.49",
    "laravel-nova-ui": "0.4.10"
  },
  "devDependencies": {
    "vue-loader": "16.8.3"
  }
}
